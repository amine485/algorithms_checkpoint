    "def bubble(nlist) :\n",
    "    for num in range(len(nlist)-1,0,-1):\n",
    "        for i in range(num):\n",
    "            if nlist[i] > nlist[i+1]:\n",
    "                nlist[i], nlist[i+1] = nlist[i+1], nlist[i]\n",
    "    return nlist\n",
    "array1 = [5,4,8,2,3,4,7,6,2]\n",
    "print(bubble(array1))\n"
   ]
